{
  "name": "wooji-js-starter-kit",
  "version": "0.0.1",
  "description": "wooji.js Starter Kit",
  "scripts": {
    "g:list": "ls $(npm root -g)",
    "g:install": "npm install -g $(iojs -p \"require('./package.json').globalDependencies.join(' ')\")",
    "g:uninstall": "npm uninstall -g $(iojs -p \"require('./package.json').globalDependencies.join(' ')\")",
    "start": "iojs --harmony_arrow_functions $(which gulp)",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooji-js/wooji-js-starter-kit.git"
  },
  "author": "ejelome <ejelome@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooji-js/wooji-js-starter-kit/issues"
  },
  "homepage": "https://github.com/wooji-js/wooji-js-starter-kit#readme",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.1",
    "browserify": "^10.2.1",
    "browserify-global-shim": "^1.0.0",
    "browserify-shim": "^3.8.7",
    "gulp": "^3.8.11",
    "vinyl-source-stream": "^1.1.0"
  },
  "globalDependencies": [
    "gulp"
  ],
  "browserify-global-shim": {
    "react": "React"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-global-shim"
    ]
  }
}
